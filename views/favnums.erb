<form method="post" action="favnums">
    <link rel="stylesheet" type="text/css" href="/css/layout.css">
    <div class="input_container">
        <%=p "What are your 3 favorite numbers?"%>
        <br>
        <input type="hidden" name="first_name" value="<%=first_name%>" class="name_input">
        <input type="hidden" name="last_name" value="<%=last_name%>" class="name_input">
        <input type="hidden" name="street" value="<%=street%>" class="street_input">
        <input type="hidden" name="city" value="<%=city%>" class="city_input">
        <input type="hidden" name="state" value="<%=state%>" class="state_input">
        <input type="hidden" name="zip" value="<%=zip%>" class="zip_input">
        <input type="number" name="num1" class="num1_input" placeholder="Favorite Number 1" required autofocus>
        <input type="number" name="num2" class="num2_input" placeholder="Favorite Number 2" required>
        <input type="number" name="num3" class="num3_input" placeholder="Favorite Number 3" required>
    </div>
    <div class="here">
    <button class="button">Submit</button>
    </div>
</form>